        android:background="@drawable/qwest_textbox_reward_small"
        android:gravity="center"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="완료 보상 : 망각곡선 자동 복습기"
        android:textColor="#ffffff"
        android:textSize="10sp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/tv_tuto_memory_teach_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_tuto_memory_finish_reward"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:alpha="0.54"
        android:gravity="center"
        android:lineSpacingExtra="6dp"
        android:text="그럼 지금부터 김기훈님의\n망각곡선 측정을 위한\n효율적인 단어 암기 방법을\n알려드릴게요"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/iv_tuto_memory_learnpage"
        android:layout_width="288dp"
        android:layout_height="288dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="168dp"
        android:src="@drawable/forgetqwest_img_learnpage"
        android:visibility="invisible" />

    <org.andlib.ui.CircleAnimationView
        android:id="@+id/bt_tuto_memory_learn"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="32dp"
        android:background="@drawable/forgetqwest_confirm_btn"
        android:clickable="false"
        android:visibility="invisible" />
<!--  -->
    <TextView
        android:id="@+id/tv_tuto_memory_right_push_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_tuto_memory_finish_reward"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:lineSpacingExtra="6dp"
        android:text="오른쪽으로 살짝 밀어서\n단어의 뜻을 보고 외워주세요"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_tuto_memory_tts_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_tuto_memory_finish_reward"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:lineSpacingExtra="6dp"
        android:text="단어를 터치하면\n원어민 발음도 들을 수 있어요!\n아마 외우는데 도움 될 거에요"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tv_tuto_memory_left_push_text"
